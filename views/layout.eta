	#$ if (render.src.style) { $#
	<link
		rel="stylesheet"
		href="#$=render.src.style$#"
	/>
	#$ } $#
</head>


<body>
	#$ if (it.render) { $#
	<div class="wrapper">
		#$~ include("partials/header", it )$#
		#$~ body $#
		#$~ include("partials/footer", it )$#
	</div>

	#$ if (render.src.script) { $#
	<script src="#$=render.src.script$#"></script>
	#$ } $#

	#$ } else { $#
	<h2 style="color: var(--red);">(◯Δ ◯ ∥)</h2>
	#$ } $#
</body>

