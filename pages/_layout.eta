	<link rel="stylesheet" href="/public/style.blocks/textarea.css" />
	<link rel="stylesheet" href="/public/style.blocks/button.css" />

	<% if (it.render && render.src && render.src.style) { %>
	<link rel="stylesheet" href="<%=render.src.style%>" />
	<% } %>
</head>

<body>
	<div class="wrapper">
		<%~ include("/blocks/header", it )%>
		<%~ body %>
		<%~ include("blocks/footer", it )%>
	</div>

	<% if (it.render && render.src && render.src.script) { %>
	<script src="<%=render.src.script%>"></script>
	<% } %>
</body>

</html>
