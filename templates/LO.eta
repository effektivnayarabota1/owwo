	<link rel="stylesheet" href="/templates/header/header.css" />
	<link rel="stylesheet" href="/templates/input/input.css" />
	<link rel="stylesheet" href="/templates/textarea/textarea.css" />

	<% if (it.render && render.src && render.src.style) { %>
	<link rel="stylesheet" href="<%=render.src.style%>" />
	<% } %>
</head>

<body>
	<div class="wrapper">
		<%~ include("header/header", it )%>
		<%~ body %>
		<%~ include("footer/footer", it )%>
	</div>

	<% if (it.render && render.src && render.src.script) { %>
	<script src="<%=render.src.script%>"></script>
	<% } %>
</body>

</html>
