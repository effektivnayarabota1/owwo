<% const text = it.text || '' %>
<% const name = it.name || 'textarea' %>
<% const type = it.type || 'default' %>
<% const rows = it.rows || '' %>

<textarea
					id="textarea"
					class="textarea border__outter"
					spellcheck="false"
					name="<%= name %>"
					placeholder="<%= placeholder %>"
					rows="<%= rows %>"><%= text %></textarea>
